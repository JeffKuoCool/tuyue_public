<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="SquareProcessView">
        <attr name="strokeColor" format="color|reference" />
        <attr name="strokeWith" format="dimension|reference" />
        <attr name="progressColor" format="color|reference" />
        <attr name="textSize" format="dimension|reference" />
        <attr name="textColor" format="color|reference" />
        <attr name="currentPogress" format="integer" />
    </declare-styleable>

</resources>